FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 2.145e-06;

boundaryField
{
    inlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    top
    {
        type            calculated;
        value           uniform 2.145e-06;
    }
    bottom
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 2.145e-06;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 2.145e-06;
    }
    procBoundary2to6
    {
        type            processor;
        value           uniform 2.145e-06;
    }
}


