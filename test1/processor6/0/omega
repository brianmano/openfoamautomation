FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 3.27103;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    top
    {
        type            slip;
    }
    bottom
    {
        type            slip;
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary6to0
    {
        type            processor;
        value           uniform 3.27103;
    }
    procBoundary6to2
    {
        type            processor;
        value           uniform 3.27103;
    }
    procBoundary6to4
    {
        type            processor;
        value           uniform 3.27103;
    }
    procBoundary6to5
    {
        type            processor;
        value           uniform 3.27103;
    }
    procBoundary6to7
    {
        type            processor;
        value           uniform 3.27103;
    }
}


