FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}

dimensions      [0 0 -1 0 0 0 0];
internalField   uniform 3.271032;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 3.271032;
    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    top
    {
        type            slip;
    }
    
    bottom
    {
        type            slip;
    }
    
    airfoil
    {
        type            omegaWallFunction;
        value           uniform 3.271032;
    }
    
    frontAndBack
    {
        type            empty;
    }
}
