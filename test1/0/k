FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}

dimensions      [0 2 -2 0 0 0 0];
internalField   uniform 0.000321;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.000321;
    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    top
    {
        type            slip;
    }
    
    bottom
    {
        type            slip;
    }
    
    airfoil
    {
        type            fixedValue;
        value           uniform 1e-12;
    }
    
    frontAndBack
    {
        type            empty;
    }
}
