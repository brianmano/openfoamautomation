FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}

dimensions      [0 1 -1 0 0 0 0];
internalField   uniform (14.6285 0.0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (14.6285 0.0 0);
    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    top
    {
        type            slip;
    }
    
    bottom
    {
        type            slip;
    }
    
    airfoil
    {
        type            noSlip;
    }
    
    frontAndBack
    {
        type            empty;
    }
}
